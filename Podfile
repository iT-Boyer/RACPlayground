  pod 'Masonry'
  pod 'XFLegoVIPER', :git => "https://github.com/yizzuide/XFLegoVIPER.git"
  pod 'MBProgressHUD', '~> 1.1.0'
  pod 'MJRefresh'
  pod 'SDWebImage', '~> 5.8.0'
  pod 'OHHTTPStubs'   # Default subspecs, including support for NSURLSession & JSON etc
  target 'RACPlaygroundTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RACPlaygroundUITests' do
    # Pods for testing
  end

end
